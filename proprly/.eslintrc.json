{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    
    "parserOptions": {
        "babelOptions": {
            "presets": [
                "@babel/preset-react"
            ]
        },
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module",
        "requireConfigFile":false
    },
    "parser": "@babel/eslint-parser",
    "plugins": [
        "react",
        "@babel"
    ],
    "rules": {
    }
}
